msgid "Could not read file '%1'"
msgstr "Не удалось открыть файл «%1»"

#: kjsembed/qformbuilder_binding.cpp:67 kjsembed/quiloader_binding.cpp:124
#, kde-format
msgid "Must supply a filename."
msgstr "Необходимо указать имя файла."


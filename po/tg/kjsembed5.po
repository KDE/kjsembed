#: kjsembed/quiloader_binding.cpp:129
#, kde-format
msgid "Could not open file '%1': %2"
msgstr "Файли '%1' кушода намешавад: %2"

#: kjsembed/quiloader_binding.cpp:137
#, kde-format
msgid "Failed to load file '%1'"
msgstr "Боркунии файли '%1' қатъ шуд"


#, kde-format
msgid "'%1' is not a valid QLayout."
msgstr "'%1' тарҳбандии дурусти QLayout намебошад."

#: kjsembed/qlayout_binding.cpp:93
#, kde-format
msgid "Must supply a layout name."
msgstr "Бояд номи тарҳбандиро таъмин намояд."

#: kjsembed/qobject_binding.cpp:141
#, kde-format
msgid "Wrong object type."
msgstr "Навъи мавод нодуруст аст."


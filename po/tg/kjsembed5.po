#, kde-format
msgid "Must supply a valid parent."
msgstr "Бояд волиди дурустро таъмин намояд."

#: kjsembed/qformbuilder_binding.cpp:56
#, kde-format
msgid "There was an error reading the file '%1'"
msgstr "Ҳангоми хондани файли '%1' хато рӯй дод"


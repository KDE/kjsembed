#: kjsembed/qobject_binding.cpp:727
#, kde-format
msgid "Call to '%1' failed."
msgstr "Дархости '%1' қатъ шуд."

#: kjsembed/qobject_binding.cpp:793
#, kde-format
msgid "Could not construct value"
msgstr "Қимат сохта намешавад"

#: kjsembed/quiloader_binding.cpp:49
#, kde-format
msgid "Not enough arguments."
msgstr "Шумораи нишонҳо кифоя нест."

#: kjsembed/quiloader_binding.cpp:68
#, kde-format
msgid "Failed to create Action."
msgstr "Эҷодкунии амал қатъ шуд."

#: kjsembed/quiloader_binding.cpp:82
#, kde-format
msgid "Failed to create ActionGroup."
msgstr "Эҷодкунии гурӯҳи амал қатъ шуд."

#: kjsembed/quiloader_binding.cpp:90
#, kde-format
msgid "No classname specified"
msgstr "Ягон номи синф муайян карда нашуд"

#: kjsembed/quiloader_binding.cpp:98
#, kde-format
msgid "Failed to create Layout."
msgstr "Эҷодкунии тарҳбандӣ қатъ шуд."

#: kjsembed/quiloader_binding.cpp:107
#, kde-format
msgid "No classname specified."
msgstr "Ягон номи синф муайян карда нашуд."

#: kjsembed/quiloader_binding.cpp:115
#, kde-format
msgid "Failed to create Widget."

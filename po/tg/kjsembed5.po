#: kjsembed/qobject_binding.cpp:661
#, kde-format
msgid "No such method '%1'."
msgstr "Усули '%1' вуҷуд надорад."

#: kjsembed/qobject_binding.cpp:690
#, kde-format
msgid "Call to method '%1' failed, unable to get argument %2: %3"

#: kjsembed/builtins.cpp:147
#, kde-format
msgid "Confirm"
msgstr "Тасдиқ кардан"

#: kjsembed/eventproxy.cpp:110
#, kde-format
msgid "Bad event handler: Object %1 Identifier %2 Method %3 Type: %4."

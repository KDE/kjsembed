msgstr[0] "Такягоҳ %1 нишонро дархост кард"
msgstr[1] "Такягоҳ %1 нишонро дархост кард"

#: kjsembed/qobject_binding.cpp:411
#, kde-format
msgid "but there is only %1 available"
msgid_plural "but there are only %1 available"
msgstr[0] "Аммо танҳо %1 дастрас аст"
msgstr[1] "Аммо танҳо %1 дастрасанд"

#: kjsembed/qobject_binding.cpp:412
#, kde-format
msgctxt ""
"%1 is 'the slot asked for foo arguments', %2 is 'but there are only bar "
"available'"
msgid "%1, %2."
msgstr "%1, %2."

